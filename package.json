{
  "name": "rcss",
  "displayName": "Syntax Highlighting for RCSS",
  "description": "Adds syntax highlighting to the RCSS language",
  "version": "0.0.2",
  "publisher": "rcss-syntax-highlighting",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "rcss",
      "aliases": ["RCSS", "rcss"],
      "extensions": [".rcss"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "rcss",
      "scopeName": "source.rcss",
      "path": "./syntaxes/rcss.tmLanguage.json"
    }]
  }
}
